{
  "name": "testtania",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-debug-note": "Run from command line as npm run <scriptname>.  Default debug port is 9229",
    "start-debug1": "react-scripts --inspect start --runInBand --no-cache",
    "start-debug2": "react-scripts --inspect=:3002 start --runInBand --no-cache",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject-donotrun": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -r https://github.com/brbruce/brbruce.github.io.git -b master --dest react-app"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": "https://brbruce.github.io/react-app"
}
